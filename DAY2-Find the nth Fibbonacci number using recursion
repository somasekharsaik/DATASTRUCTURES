#include <stdio.h>

int fibonacci(int n) {
    if (n <= 1)
        return n;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n = 10; // Change n to the desired Fibonacci number position
    printf("Fibonacci number at position %d is: %d", n, fibonacci(n));
printf("regno:192324134\n");
    return 0;
}
