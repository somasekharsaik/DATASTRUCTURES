#include <stdio.h>

int main() {
    char alphabet[26];
    int i;

    for (i = 0; i < 26; i++) {
        alphabet[i] = 'A' + i;
    }
printf("reg.no:192324134\n");
    printf("Ascending Order:\n");
    for (i = 0; i < 26; i++) {
        printf("%c ", alphabet[i]);
    }

    printf("\nDescending Order:\n");
    for (i = 25; i >= 0; i--) {
        printf("%c ", alphabet[i]);
    }

    return 0;
}
